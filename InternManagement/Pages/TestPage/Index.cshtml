@page
@model InternManagement.Pages.TestPage.IndexModel
@{
    ViewData["Tittle"] = "Testing page";
}
<div class="text-center">
    <h1 class="display-4">File Upload</h1>
</div>
<div class="container">
    <div class="mb-3 mt-3">
        <form method="post" enctype="multipart/form-data" asp-controller="TestPage" asp-action="Index">
            <div class="mb-3">
                <label class="form-label">Upload File</label>
                <input type="file" name="file" class="form-control"/>
            </div>
            <div class="mb-3">
                <input class="btn btn-success" type="submit" value="Upload" />
            </div>
        </form>
    </div>
</div>