@model IMSBussinessObjects.Intern

<div>
    <h3>@Model.Name</h3>
    <p><strong>Email:</strong> @Model.Email</p>
    <p><strong>Phone Number:</strong> @Model.PhoneNumber</p>
    <p><strong>Personal ID:</strong> @Model.PersonalId</p>
    <p><strong>University:</strong> @Model.Uni</p>
    <p><strong>Major:</strong> @Model.Major</p>
    <p><strong>Grade:</strong> @Model.Grade</p>
    <p><strong>GPA:</strong> @Model.Gpa</p>
    <p><strong>Birthday:</strong> @Model.Birthday.ToShortDateString()</p>
    <p><strong>Start Date:</strong> @Model.StartDate.ToShortDateString()</p>
    <p><strong>End Date:</strong> @Model.EndDate.ToShortDateString()</p>
    <p><strong>Team:</strong> @Model.Team.Name</p>
    <p><strong>Status:</strong> @Model.Status</p>
</div>
